<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.bean.report.form">
	<class name="CustomerVolumeByBranch" table="CUSTOMER_VOLUME_BY_BRANCH">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="string" column="REC_id"></id>
		<property name="searchDate" column="SEARCH_DATE" type="string" not-null="false" length="10"/>
		<property name="branch" column="BRANCH" type="string" not-null="false" length="10"/>
		<property name="branchName" column="BRANCH_NAME" type="string" not-null="false" length="320"/>
		<property name="customerVolumeWpb" column="CUSTOMER_VOLUME_WPB" type="big_decimal" not-null="false" length="20"/>
		<property name="customerVolumeGbm" column="CUSTOMER_VOLUME_GBM" type="big_decimal" not-null="false" length="20"/>
		<property name="customerVolumeCmb" column="CUSTOMER_VOLUME_CMB" type="big_decimal" not-null="false" length="20"/>
		<property name="customerVolumeAll" column="CUSTOMER_VOLUME_ALL" type="big_decimal" not-null="false" length="20"/>
	</class>	
</hibernate-mapping>