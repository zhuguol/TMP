<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.export">
	<class name="AmlUboReportInfo" table="AML_UBO_REPORT_INFO">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="string" column="SEQ_NO"></id>
		<property name="dataDate" column="DATA_DATE" type="string" not-null="false" length="6" />
		<property name="custNo" column="CUST_NO" type="string" not-null="false" length="9" />
		<property name="custName" column="CUST_NAME" type="string" not-null="false" length="512" />
		<property name="pName" column="P_NAME" type="string" not-null="false" length="512" />
		<property name="pIdty" column="P_IDTY" type="string" not-null="false" length="18" />
		<property name="pIdno" column="P_IDNO" type="string" not-null="false" length="128" />
		<property name="pIdep" column="P_IDEP" type="string" not-null="false" length="8" />
		<property name="addresss" column="ADDRESSS" type="string" not-null="false" length="4000" />
		<property name="lob" column="LOB" type="string" not-null="false" length="24" />
	</class>
</hibernate-mapping>