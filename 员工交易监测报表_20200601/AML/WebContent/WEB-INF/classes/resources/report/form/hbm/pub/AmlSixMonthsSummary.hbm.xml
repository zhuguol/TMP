<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.bean.report.form">
	<class name="AmlSixMonthsSummary" table="AML_SIX_MONTHS_SUMMARY">
		<meta attribute="sync-DAO">false</meta>
		<id name="seqNo" type="big_decimal" column="SEQ_NO"></id>
		<property name="dataNo" column="DATA_NO" type="string" not-null="false" length="60"/>
		<property name="dataDate" column="DATA_DATE" type="string" not-null="false" length="12"/>
		<property name="levelNo" column="LEVEL_NO" type="string" not-null="false" length="120"/>
		<property name="levelName" column="LEVE_NAME" type="string" not-null="false" length="120"/>
		<property name="sccToFcr" column="SCC_TO_FCR" type="big_decimal" not-null="false" length="32"/>
		<property name="sccToMis" column="SCC_TO_MIS" type="big_decimal" not-null="false" length="32"/>
		<property name="sccToSafe" column="SCC_TO_SAFE" type="big_decimal" not-null="false" length="32"/>
		<property name="sccTotal" column="SCC_TOTAL" type="big_decimal" not-null="false" length="32"/>
		<property name="dataFlag" column="DATA_FLAG" type="string" not-null="false" length="12"/>
	</class>	
</hibernate-mapping>