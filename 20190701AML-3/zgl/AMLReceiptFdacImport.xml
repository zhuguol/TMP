<?xml version="1.0" encoding="UTF-8"?>

<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号 transdataactionurl:数据整理URL txnlogflag:true记录日志 async:异步提交标志
	pagesize:页面显示记录数(缺省为10) -->
<CommQry title="回执文件" navigate="回执文件导入" type="call" interface="false"
	pagesize="100" async="true" databusid="" transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true" getterclassname="com.huateng.report.hfaml3.getter.AMLWarmReceiptImportGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示 -->
		<!-- primary : true-作为表单项记录上传 false-不作为表单项上传 -->
		<Field id="id" desc="id"  method="None" edittype="text" datatype="string" xpath="/id" />
		
		<Field id="importTime" desc="导入时间" edittype="text" datatype="timestamp" method="None" readonly="false" 
		       size="14" status="F" default="" xpath="/importTime" />
		       
		<Field id="importTlr" desc="导入操作员" edittype="text" datatype="string" method="None" readonly="false" 
		       size="20" status="F" default="" xpath="/importTlr" />
		
		<Field id="brno" desc="所属机构号" edittype="text" datatype="string" method="None" readonly="false" 
		       size="20" status="F" default="" xpath="/brno" />
		
		<Field id="fileName" desc="回执文件名称" edittype="text" status="F"
			datatype="string" size="200" xpath="/fileName" />
	</Fields>

	<Where>
        <TextBox id="qworkdateStart" desc="开始上传日期"  require="true"
			status="F" datatype="date" method="None" size="" width="" primary="false"
			index="0" type="String" />
		<TextBox id="qworkdateEnd" desc="结束上传日期" require="true"
			status="F" datatype="date" method="None" size="" width="" primary="false"
			index="1" type="String" />
		<TextBox id="qtlrno" desc="上传人" require="false"
			status="F" datatype="string" method="None" size="" width="" primary="false"
			index="2" type="String" />
	</Where>
	<Operations>
		<Button id="btnImportReceipt" name="btnImportReceipt" desc="导入回执" url="#" show="" group="" txn=""  icon="icon-impin"  />
	</Operations>

</CommQry>