<?xml version="1.0" encoding="UTF-8"?>

<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号 transdataactionurl:数据整理URL txnlogflag:true记录日志 async:异步提交标志
	pagesize:页面显示记录数(缺省为10) -->
<CommQry title="文件上传" navigate="文件上传" type="call" interface="false"
	pagesize="100" async="true" databusid="" transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true" getterclassname="com.huateng.hsbc.cust.getter.CustUploadFileInfoGetter">
	<Include id="BankParam" />
	
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示 -->
		<!-- primary : true-作为表单项记录上传 false-不作为表单项上传 -->
		<Field id="select" desc="选择" method="None" edittype="checkbox" datatype="boolean" xpath="" />
		<Field id="ftloperation" desc="操作" status="N" size="20" tip="操作"/>
		<Field id="id" desc="id"  method="None" edittype="text" datatype="string" xpath="/id" />
		<Field id="batchNo" desc="批次号" edittype="text" datatype="string" method="None" readonly="true" size="20"  status="F" default="" xpath="/batchNo"/>
		<Field id="certNum" desc="证件号" edittype="text" datatype="string" method="None" readonly="true" size="20"  status="F" default="" xpath="/certNum"/>
		<Field id="name" desc="姓名/名称" edittype="text" datatype="string" method="None" readonly="true" size="256"  status="F" default="" xpath="/name"/>
		<Field id="uploadTlr" desc="上传人ID" edittype="text" datatype="string" method="None" readonly="true" size="20"  status="F" default="" xpath="/uploadTlr"/>
		<Field id="uploadTime" desc="上传时间" edittype="text" datatype="timestamp" method="None" readonly="true" size="14"  status="F" default="" xpath="/uploadTime"/>
		<Field id="uploadFileName" desc="上传文件名称" edittype="text" datatype="string" method="None" readonly="true" size="100"  status="F" default="" xpath="/uploadFileName"/>
	</Fields>

	<Where>
		<TextBox id="qDateStart" desc="开始日期"  require="true" status="F" datatype="date" method="None" size="" width="" primary="false" index="0" type="String" />
		<TextBox id="qDateEnd" desc="结束日期" require="true" status="F" datatype="date" method="None" size="" width="" primary="false" index="1" type="String" />
		<TextBox id="qbatchNo" desc="批次号" require="false" status="F" datatype="string" method="None" size="" width="" primary="false" index="2" type="String" />
	</Where>
	<Operations>
		<Button id="btnUpload" desc="导入" url="#" name="btnUpload" icon="icon-impin" txn="" group="" show=""/>
	</Operations>
</CommQry>