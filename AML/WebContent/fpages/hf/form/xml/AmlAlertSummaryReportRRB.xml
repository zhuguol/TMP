<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="员工交易监测日报" navigate="员工交易监测功能&gt;员工交易监测日报"
	type="call" 
	interface="true" 
	pagesize="100" 
	async="true" 
	databusid=""
	transdataactionurl="/trans/TransDataAction.do" 
	getterclassname="com.huateng.report.hf.form.getter.AmlAlertSummaryReportRRBGetter">
	<Include id="BankParam" />
	<Fields>
		<Field id="seqNo" desc="序号" require="false" edittype="text" datatype="string" method="None" readonly="false" size="30" status="N" default="" primary="" xpath="/seqNo" />
		<Field id="id" desc="指标" require="false" edittype="text" datatype="string" method="None" readonly="false" size="12" status="N" default="" primary="" xpath="/id" />
		<Field id="idDesc" desc="指标描述" require="false" edittype="text" datatype="string" method="None" readonly="false" size="128" status="N" default="" primary="" xpath="/idDesc" />
		<Field id="sumNum" desc="统计值" require="false" edittype="text" datatype="string" method="None" readonly="false" size="40" status="N" default="" primary="" xpath="/sumNum" />
		<Field id="dataDate" desc="统计日期" require="false" edittype="text" datatype="string" method="None" readonly="false" size="6" status="N" default="" primary="" xpath="/dataDate" />
		<Field id="idName" desc="指标名称" require="false" edittype="text" datatype="string" method="None" readonly="false" size="128" status="N" default="" primary="" xpath="/idName" />
	</Fields>
	<Where>
		<Option id="etlDateStart" desc="统计起始日期(年/月)" placeholder="yyyyMM"
			edittype="select" dropdowntype="custom" url="/templets/lib/yyyyMM.ftl"
			rules="isNum" editable="true" size="6" status="F" require="true"
			fieldmap="etlDateStart=yyyyMM;etlDateStartName=yyyyMM" viewField="yyyyMM"
			index="0" />
		<Option id="etlDateEnd" desc="统计结束日期(年/月)" placeholder="yyyyMM"
			edittype="select" dropdowntype="custom" url="/templets/lib/yyyyMM.ftl"
			rules="isNum" editable="true" size="6" status="F" require="true"
			fieldmap="etlDateEnd=yyyyMM;etlDateEndName=yyyyMM" viewField="yyyyMM"
			index="1" />
	</Where>
	<Operations>
		<Button id="btnDownLoad" name="btnDownLoad" desc="数据下载"  operation="" url="#" show="" group="" txn="" icon="icon-export-xls"/>
	</Operations>
</CommQry>