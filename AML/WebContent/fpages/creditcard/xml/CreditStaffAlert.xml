<?xml version="1.0" encoding="UTF-8"?>

<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号 transdataactionurl:数据整理URL txnlogflag:true记录日志 async:异步提交标志
	pagesize:页面显示记录数(缺省为10) -->
<CommQry title="信用卡告警员工相关报表" navigate="信用卡告警员工相关报表" type="call" interface="false"
	pagesize="10" async="true" databusid="" transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true" getterclassname="com.huateng.hsbc.creditcard.getter.CreditStaffAlertGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示 -->
		<!-- primary : true-作为表单项记录上传 false-不作为表单项上传 -->
		<Field id="rec_id" desc="id"  method="None" edittype="text" datatype="string" xpath="/rec_id" />
		<Field id="alarm_date" desc="Bacth Cycle(批次号)" edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/alarm_date" width="200"/>
		<Field id="alarm_no"               desc="ALERTID"                          edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/alarm_no"               width="200"/>
		<Field id="cust_no"                desc="客户号"                           edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/cust_no"                width="200"/>
		<Field id="cust_name"              desc="客户名称"                         edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/cust_name"              width="200"/>
		<Field id="cust_cert_type"         desc="证件类型"    edittype="select" translated="DATA_DIC.1014" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/cust_cert_type"         width="200"/>
		<Field id="cust_cert_no"           desc="证件号码"                         edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/cust_cert_no"           width="200"/>
		<Field id="cust_category"          desc="客户分类（Advanced/Permier/Jade)" edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/cust_category"          width="200"/>
		<Field id="cust_phone_no"          desc="手机号"                           edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/cust_phone_no"          width="200"/>
		<Field id="cust_work_unit"         desc="公司"                             edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/cust_work_unit"         width="200"/>
		<Field id="cust_income"            desc="收入"                             edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/cust_income"            width="200"/>
		<Field id="credit_card_cny"        desc="信用卡卡号（人民币）"             edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/credit_card_cny"        width="200"/>
		<Field id="active_time_cny"        desc="卡片激活时间（人民币）"           edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/active_time_cny"        width="200"/>
		<Field id="credit_card_usd"        desc="信用卡卡号（美元）"               edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/credit_card_usd"        width="200"/>
		<Field id="active_time_usd"        desc="卡片激活时间（美元）"             edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/active_time_usd"        width="200"/>
		<Field id="limit_cny"              desc="信用额度（人民币）"               edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/limit_cny"              width="200"/>
		<Field id="limit_available_cny"    desc="可用额度（人民币）"               edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/limit_available_cny"    width="200"/>
		<Field id="billing_date"           desc="账单日"                           edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/billing_date"           width="200"/>
		<Field id="billing_address"        desc="账单地址"                         edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/billing_address"        width="200"/>
		<Field id="repay_date"             desc="还款日"                           edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/repay_date"             width="200"/>
		<Field id="first_reduce_time"      desc="最近一次降额时间"                 edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/first_reduce_time"      width="200"/>
		<Field id="transactor_no"          desc="客户开卡经办销售工号"             edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/transactor_no"          width="200"/>
		<Field id="proof_result"           desc="凭证验证结果"                     edittype="select" translated="DATA_DIC.1000"  datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/proof_result"           width="200"/>
		<Field id="merchant_result"        desc="商户验证结果"                     edittype="select"  translated="DATA_DIC.1001"  datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/merchant_result"        width="200"/>
		<Field id="outbound_result"        desc="外呼结果"                         edittype="select"  translated="DATA_DIC.1002"  datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/outbound_result"        width="200"/>
		<Field id="investigate_result"     desc="调查结果"                         edittype="select"  translated="DATA_DIC.1003" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/investigate_result"     width="200"/>
		<Field id="investigate_report"     desc="调查报告"                         edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/investigate_report"     width="200"/>
		<Field id="staff_no"               desc="员工号"                           edittype="text" datatype="string" method="None" readonly="true" size="512"  status="F" default="" xpath="/staff_no"               width="200"/>

	</Fields>

	<Where>
		<Option id="qlstDate" desc="Closed Alert Month" placeholder="yyyyMM"
			edittype="select" dropdowntype="custom" url="/templets/lib/yyyyMM.ftl"
			rules="isNum" editable="true" size="6" status="F" require="false"
			fieldmap="qlstDate=yyyyMM;qlstDateName=yyyyMM" viewField="yyyyMM"
			index="0" />
    </Where>
	<Operations>
		<Button id="btDownload" name="btDownload" desc="导出" updateClass="" operation="" url="#" show="" group="" txn="" icon="icon-export-xls" />
	</Operations>

</CommQry>