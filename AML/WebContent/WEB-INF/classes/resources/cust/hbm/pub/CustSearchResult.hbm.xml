<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.hsbc.cust.bean">
	<class
		name="CustSearchResult"
		table="CUST_SEARCH_RESULT"
	>
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="string" column="REC_ID">
		</id>

		<property name="batchNo"    column="BATCH_NO"    type="string" not-null="false" length="20" />
		<property name="uploadDate"  column="UPLOAD_DATE"    type="string" not-null="false" length="8" />
		<property name="type"    column="TYPE"    type="string" not-null="false" length="20" />
		<property name="certNum"    column="CERT_NUM"    type="string" not-null="false" length="20" />
		<property name="name"    column="NAME"    type="string" not-null="false" length="256" />
		<property name="custNo"    column="CUST_NO"    type="string" not-null="false" length="20" />
		<property name="custName"    column="CUST_NAME"    type="string" not-null="false" length="256" />
		<property name="legalCertNo"    column="LEGAL_CERT_NO"    type="string" not-null="false" length="20" />
		<property name="legalName"    column="LEGAL_NAME"    type="string" not-null="false" length="256" />
		<property name="mainId"    column="MAIN_ID"    type="string" not-null="false" length="32" />
		<property name="rsv1"              column="RSV1"                type="string" not-null="false" length="10"  />
		<property name="rsv2"              column="RSV2"                type="string" not-null="false" length="10"  />
		<property name="rsv3"              column="RSV3"                type="string" not-null="false" length="50"  />
		<property name="rsv4"              column="RSV4"                type="string" not-null="false" length="50"  />
		<property name="rsv5"              column="RSV5"                type="string" not-null="false" length="500" />
		<property name="rsv6"              column="RSV6"                type="string" not-null="false" length="500" />	
	</class>	
</hibernate-mapping>