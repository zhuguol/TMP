<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.bean.report.form">
	<class name="AmlCustMxInfo" table="AML_CUST_MX_INFO">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="string" column="ID"></id>
		<property name="custNum"  column="CUST_NUM" type="string" not-null="false" length="20"/>
		<property name="tradeNum"  column="TRADE_NUM" type="string" not-null="false" length="20"/>
		<property name="tradeType"  column="TRADE_TYPE" type="string" not-null="false" length="20"/>
		<property name="tradeDate"  column="TRADE_DATE" type="string" not-null="false" length="20"/>
		<property name="filler1"  column="FILLER1" type="string" not-null="false" length="20"/>
		<property name="filler2"  column="FILLER2" type="string" not-null="false" length="20"/>
		<property name="filler3"  column="FILLER3" type="string" not-null="false" length="20"/>
	</class>	
</hibernate-mapping>