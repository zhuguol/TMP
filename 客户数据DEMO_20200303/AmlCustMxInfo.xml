<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="大额交易" navigate="反洗钱&gt;  大额交易"
	type="call" 
	interface="true" 
	pagesize="100" 
	async="true" 
	databusid=""
	transdataactionurl="/trans/TransDataAction.do" 
	getterclassname="com.huateng.report.hf.form.getter.AmlCustMxInfoGetter">
	<Include id="BankParam" />
	<Fields>
		<Field id="id" desc="记录主键" edittype="text" datatype="string" method="None" readonly="false" size="20" status="N"  primary="true" xpath="/id" />
		<Field id="custNum" desc="客户编号" edittype="text" datatype="string" require="false" method="None" readonly="false" size="20" status="F"  xpath="/custNum" /> 
		<Field id="tradeNum" desc="交易编号" edittype="text" datatype="string" method="None" readonly="false" size="20" status="F" default="" xpath="/tradeNum" />
		<Field id="tradeType" desc="交易类型" edittype="text" datatype="string" method="None" readonly="false" size="20" status="F" default="" xpath="/tradeType" />
		<Field id="tradeDate" desc="交易时间" edittype="text" datatype="string" method="None" readonly="false" size="20" status="F" default="" xpath="/tradeDate" />
		<Field id="filler1" desc="备用字段1" edittype="text" datatype="string" method="None" readonly="false" size="20" status="F" default="" xpath="/filler1" />
		<Field id="filler2" desc="备用字段2" edittype="text" datatype="string" method="None" readonly="false" size="20" status="F" default="" xpath="/filler2" />
		<Field id="filler3" desc="备用字段3" edittype="text" datatype="string" method="None" readonly="false" size="20" status="F" default="" xpath="/filler3" />
	</Fields>
	<Where>
		<DateTextBox id="etlDateStart" desc="交易起始日期" edittype="text" datatype="date" size="34" require="false" index="0" />
		<DateTextBox id="etlDateEnd" desc="交易终止日期" edittype="text" datatype="date" size="34" require="false" index="1" />
		<TextBox id="tradeNums" desc="交易编号" edittype="text" status="F" datatype="string" size="20" index="2" />
		<TextBox id="tradeTypes" desc="交易类型" edittype="text" status="F" datatype="string" size="20" index="3" />
	</Where>
	<Operations>
		<Button id="btChecker" name="btChecker" desc="是否再查" updateClass="" operation="" url="#" show="" group="" txn="" icon="icon-default" />
		<Button id="btCancel" name="btCancel" desc="返回" updateClass="" operation="" url="#" show="" group="" txn="" icon="" />
	</Operations>
</CommQry>