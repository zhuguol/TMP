<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.bean.report.form">
	<class name="AmlCustInfo" table="AML_CUST_INFO">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="string" column="ID"></id>
		<property name="custName"  column="CUST_NAME" type="string" not-null="false" length="20"/>
		<property name="custNum"  column="CUST_NUM" type="string" not-null="false" length="20"/>
		<property name="custZjnum"  column="CUST_ZJNUM" type="string" not-null="false" length="64"/>
		<property name="custStatus"  column="CUST_STATUS" type="string" not-null="false" length="8"/>
	</class>	
</hibernate-mapping>