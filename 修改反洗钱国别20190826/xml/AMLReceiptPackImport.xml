<?xml version="1.0" encoding="UTF-8"?>

<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号 transdataactionurl:数据整理URL txnlogflag:true记录日志 async:异步提交标志
	pagesize:页面显示记录数(缺省为10) -->
<CommQry title="回执文件" navigate="回执文件导入" type="call" interface="false"
	pagesize="100" async="true" databusid="" transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true" getterclassname="com.huateng.report.aml.feedback.getter.AmlSubFileInfoGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示 -->
		<!-- primary : true-作为表单项记录上传 false-不作为表单项上传 -->

        
        <Field id="id" desc="上报的文件名称" edittype="select" status="F"
			datatype="string" primary="true" size="20" translated="DATA_DIC.17"
			xpath="/id" />
			
		<Field id="busiType" desc="业务类型" edittype="select" status="F"
			datatype="string" primary="true" size="20" translated="LIST:02,反洗钱"
			xpath="/busiType" />
				
		<Field id="apptype" desc="应用类型" edittype="select" status="F"
			datatype="string" primary="true" size="20" translated="LIST:AML,AML-反洗钱"
			xpath="/apptype" />
			
		 <Field id="workdate" desc="工作日期" edittype="text" datatype="date" method="None" readonly="false" 
		       size="8" status="F" default="" xpath="/workdate" />
		       
		<Field id="currentfile" desc="文件类型" edittype="select" status="F"
			datatype="string" primary="true" size="20" translated="DATA_DIC.150"
			xpath="/currentfile" />

		<Field id="filePack" desc="文件包" edittype="text" status="F"
			datatype="string" primary="true" size="64" xpath="/filePack" />

		<Field id="reportType" desc="报文类型" edittype="select" status="F"
			datatype="string"    translated="DATA_DIC.9021"   primary="true"   size="2"
			xpath="/reportType" />

		<Field id="subType" desc="报送方式" edittype="select" status="F"
			datatype="string"    translated="DATA_DIC.90"   primary="true"   size="2"
			xpath="/subType" />

		<Field id="crtTm" desc="打包时间" require="" edittype="text"
			datatype="timestamp" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/crtTm" />
			
			
		<Field id="totalrecords" desc="报文交易总数" edittype="text" status="F"
			datatype="string" primary="true" size="64" xpath="/totalrecords" />
			
			
		<Field id="subTm" desc="上报时间" require="" edittype="text"
			datatype="timestamp" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/subTm" />

		<Field id="isImpRep" desc="是否已导入回执" edittype="select" status="F"
			datatype="string"    translated="LIST:Y,已导入;N,未导入"   primary="true"   size="2"
			xpath="/isImpRep" />
			
		<Field id="isSub" desc="是否已上报" edittype="select" status="F"
			datatype="string"    translated="DATA_DIC.8000"   primary="true"   size="2"
			xpath="/isSub" />	
			
		<Field id="repFileName" desc="回执文件名称" edittype="text" status="F"
			datatype="string" primary="true" size="64" xpath="/repFileName" />
			
		<Field id="repTm" desc="回执生成时间" require="" edittype="text"
			datatype="timestamp" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/repTm" />
			
		<Field id="repErrType" desc="回执文件类型" edittype="select" status="F"
			datatype="string"    translated="DATA_DIC.9024"   primary="true"   size="2"
			xpath="/repErrType" />
			
		<Field id="backfilename" desc="文件包名称" edittype="text" status="F"
			datatype="string" primary="true" size="64" xpath="" />
	</Fields>

	<Parameters></Parameters>
	<Where>
		<!-- 
		<Option id="qbusiType" desc="业务类型" datatype="string" require="true" index="0" readonly="true" translated="LIST:02,反洗钱;" />
		<Option id="qappType" desc="业务类型" datatype="string" require="true" index="1" readonly="true" translated="LIST:AML,AML-反洗钱;"/>
         -->
        <TextBox id="qworkdateStart" desc="开始工作日期"  require="true"
			status="F" datatype="date" method="None" size="" width="" primary="false"
			index="0" type="String" />
		<TextBox id="qworkdateEnd" desc="结束工作日期" require="true"
			status="F" datatype="date" method="None" size="" width="" primary="false"
			index="1" type="String" />
		<TextBox id="qcrtTm" desc="打包时间" require="false"
			status="F" datatype="date" method="None" size="" width="" primary="false"
			index="2" type="String" />
		<Option id="qcurrentfile" desc="文件类型" edittype="select" status="F" translated="DATA_DIC.150" datatype="string" size="2" index="3" />
	</Where>
	<Operations>
		<Button id="executeImport"  name="executeImport" icon="icon-impin" desc="执行导入"  operation="asysubmit"  updateClass="com.huateng.report.aml.feedback.update.AMLFeedBackHandle"
		        url="#"/>
		<Button id="btnUpload" name="btnUpload" desc="导入回执" url="#" show="" group="" txn=""  icon="icon-impin"  />
		<Button id="btnImport" name="btnImport" desc="导入人工补正" url="#" show="" group="" txn=""  icon="icon-impin"  />
	</Operations>
	<Script></Script>

</CommQry>