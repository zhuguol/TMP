<?xml version="1.0" encoding="UTF-8"?>

<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号 transdataactionurl:数据整理URL txnlogflag:true记录日志 async:异步提交标志
	pagesize:页面显示记录数(缺省为10) -->
<CommQry title="上报文件包查询" navigate="上报文件包查询" type="call" interface="false"
	pagesize="100" async="true" databusid="" transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true" getterclassname="com.huateng.report.hf.aml.getter.QuerySubFilePackageGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示 -->
		<!-- primary : true-作为表单项记录上传 false-不作为表单项上传 -->

		<Field id="id" desc="文件名称" edittype="text" datatype="string"
			primary="true" size="32" xpath="/id" />

		<Field id="apptype" desc="应用类型" edittype="select" status="F"
			datatype="string" primary="true" size="20" translated="LIST:AML,AML-反洗钱;"
			xpath="/apptype" />

		<Field id="currentfile" desc="文件类型" edittype="select" status="F"
			datatype="string" primary="true" size="20" translated="DATA_DIC.150"
			xpath="/currentfile" />

		<Field id="brNo" desc="机构号" edittype="text" status="F" datatype="string"
			primary="true" size="20" xpath="/brNo" />

		<Field id="workdate" desc="交易日期" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/workdate" />

		<Field id="crtTm" desc="生成时间" require="" edittype="text"
			datatype="timestamp" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/crtTm" />

		<Field id="repFileName" desc="回执文件名称" edittype="text" status="F"
			datatype="string" primary="true" size="64" xpath="/repFileName" />

		<Field id="repTm" desc="导入时间" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/repTm" />

		<Field id="filePack" desc="文件包" edittype="text" status="F"
			datatype="string" primary="true" size="64" xpath="/filePack" />

		<Field id="repErrType" desc="回执错误类型" edittype="text" status="F"
			datatype="string" primary="true" size="64"
			xpath="/repErrType" />

		<Field id="totalrecords" desc="总记录数" edittype="text" status="F"
			datatype="int" primary="true" xpath="/totalrecords" />

		<Field id="sucrecords" desc="成功记录数" edittype="text" status="F"
			datatype="int" primary="true" xpath="/sucrecords" />

		<Field id="falrecords" desc="失败记录数" edittype="text" status="F"
			datatype="int" primary="true" xpath="/falrecords" />

		<Field id="isImpRep" desc="是否导入回执" edittype="select" status="F"
			datatype="string"    translated="LIST:Y,已导入;N,未导入"   primary="true"   size="2"
			xpath="/isImpRep" />
		<!-- 
		<Field id="currentFileName1" desc="文件类型"  edittype="text" method="com.huateng.ebank.business.common.CQMethod.getCurrentFile"
			datatype="string" status="F" xpath="/apptype|'_'|/currentfile" />
		 -->
		<Field id="isDel" desc="是否删除" edittype="text" datatype="string" xpath="/isDel"/>
		 
		<Field id="xmlFileCount" desc="XML数量" edittype="text" datatype="string" xpath="/xmlFileCount"/>
		<Field id="oper" desc="操作" method="None" edittype="text" datatype="string" xpath="" />
	</Fields>

	<Parameters></Parameters>
	<Where>
	   <Option id="qcurrentfile" desc="交易报告类型" datatype="string" require="false" index="0" readonly="true" translated="DATA_DIC.150"/>
	   <DateTextBox id="beginCrtime" desc="文件生成起始日期" tip="工作日期" status="F" datatype="date" 
	                  method="None" size="150" width="" primary="false" index="1" type="String" />
	   <DateTextBox id="endCrtime" desc="文件生成截止日期" tip="工作日期" status="F" datatype="date" 
	                  method="None" size="150" width="" primary="false" index="2" type="String" />
	   <DateTextBox id="beginWorkDate" desc="交易起始日期" tip="工作日期" status="F" datatype="date" 
	                  method="None" size="150" width="" primary="false" index="3" type="String" />
	   <DateTextBox id="endWorkDate" desc="交易截止日期" tip="工作日期" status="F" datatype="date" 
	                  method="None" size="150" width="" primary="false" index="4" type="String" />
	   <Option id="qreportType" desc="报文类型" datatype="string" index="5"  translated="DATA_DIC.152"/>
	</Where>
	<Script></Script>

</CommQry>