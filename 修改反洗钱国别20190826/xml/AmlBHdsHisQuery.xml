<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="大额交易" navigate="反洗钱&gt;  大额交易"
	type="call" 
	interface="true" 
	pagesize="100" 
	async="true" 
	databusid=""
	transdataactionurl="/trans/TransDataAction.do" 
	getterclassname="com.huateng.report.hf.aml.getter.AmlBHdsHisQueryGetter">
	<Include id="BankParam" />
	
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示 -->
		<!-- primary : true-作为表单项记录上传 false-不作为表单项上传 -->
		<!--公共基础信息 -->
			
		<!--客户信息 -->
		<Field id="select" desc="选择" method="None" edittype="checkbox" datatype="boolean" xpath="" />	
		<Field id="ctnm" desc="客户名称/姓名" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/ctnm" tip="客户名称/姓名"/>
		<Field id="csnm" desc="客户号" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="32" status="F" default="" xpath="/csnm" />
		<Field id="roleName" desc="部门" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="30" status="F" default="" xpath="/roleName" />	
		<Field id="workDate" desc="工作日期" edittype="text" datatype="date" method="None" readonly="false" 
		       size="8" status="F" default="" xpath="/workDate" />
		<Field id="ticd" desc="业务标示号" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/ticd" tip=""/>       
		<Field id="num" desc="上报大额数量" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/num" tip="上报大额数量"/>  
		<Field id="filler1" desc="提交状态" edittype="select"  translated="DATA_DIC.8000" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/filler1" tip="提交状态" />  
		<Field id="percent" desc="完成审核比例" edittype="process" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/percent" tip="完成审核比例"/> 
		 
	</Fields>
	<Where>
		<DateTextBox id="qworkDate" desc="工作日期" edittype="text" datatype="date" size="32" require="true" index="0" />
		<TextBox id="qcsnm" desc="客户号" edittype="text" status="F" datatype="string" size="32" index="1" />
		<TextBox id="ticd" desc="业务标示号" edittype="text" status="F" datatype="string" size="32" index="1" />
	</Where>
	<Operations>
		<Button id="btSubmit" name="btSubmit" desc="确认提交" updateClass="com.huateng.report.hf.aml.update.AmlBHdsBakUpdate" operation="asysubmit" 
			url="#" show="" group="" txn="" icon="icon-ok" />
		<Button id="ctProportion" name="ctProportion" desc="计算审核占比" updateClass=""  operation="" url="#" show="" group="" txn="" icon="icon-undo" />
	</Operations>
</CommQry>