<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="可疑数据补录" navigate="可疑数据补录&gt;可疑数据补录" type="call"
	interface="true" pagesize="100" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.hf.aml.getter.AmlBSdsCollInfoGetter2">
	<Include id="BankParam" />
	<Fields>
		<!-- Common Field start -->
		<Field id="id" desc="记录主键" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/id" />
       <Field id="oper" desc="操作" method="None" edittype="text"
            datatype="string" xpath="" />
       <Field id="workDate" desc="工作日期" edittype="text" datatype="date" method="None" readonly="true" require="true"
		       size="8" status="F" default="" xpath="/workDate" />
       <Field id="reportId" desc="内部编号" edittype="text" datatype="string" require="true" method="None" readonly="false" 
		       size="10" status="F" default="" xpath="/reportId"/>   
	   <Field id="reportType" desc="可疑报文类型" edittype="select" require="true" datatype="string" method="None" readonly="true" 
		       size="2" status="F" default="" xpath="/reportType" translated="DATA_DIC.9023" />  
		<!-- 报文基本信息 -->       
		<Field id="ricd" desc="报告机构编码" edittype="text" datatype="string" require="false" method="None" readonly="true" 
		       size="16" status="F" default="" xpath="/ricd" /> 
		<Field id="rpnc" desc="上报网点代码" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="16" status="F" default="" xpath="/rpnc" />         
		<Field id="detr" desc="可疑交易报告紧急程度" require="false" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			translated="DATA_DIC.9100" default="" xpath="/detr" />     
		<Field id="torp" desc="报送次数标志" edittype="text" datatype="string" require="false" method="None" readonly="true" 
		       size="5" status="F" default="" xpath="/torp" />     
		<Field id="dorp" desc="报送方向" require="false" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			translated="DATA_DIC.9000" default="" xpath="/dorp" /> 
		<Field id="odrp" desc="其他报送方向" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="5" status="F" default="" xpath="/odrp" />   
		<Field id="tptr" desc="可疑交易报告触发点" require="false" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			translated="DATA_DIC.9101" default="" xpath="/tptr" />       
		<Field id="otpr" desc="其他可疑交易报告触发点" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="2000" status="F" default="" xpath="/otpr" /> 
		<Field id="stcb" desc="资金交易及客户行为情况" require="false" edittype="textarea"
			datatype="string" method="None" readonly="false" size="4000" status="F"
			width="735" default="" xpath="/stcb" />
		<Field id="aosp" desc="疑点分析" require="false" edittype="textarea"
			datatype="string" method="None" readonly="false" size="4000" status="F"
			width="735" default="" xpath="/aosp" />            
		 <Field id="stcr" desc="可疑特征" require="true" edittype="text" 
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" xpath="/stcr" />
		<Field id="tosc" desc="疑似涉罪类型" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" xpath="/tosc" /> 
		<Field id="rpnm" desc="填报人" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" xpath="/rpnm" />     
		       
		       
		       
		 <!-- 3号令没有用到的字段 -->       
		<Field id="finm" desc="金融机构名称" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="96" status="F"
			default="" xpath="/finm" />
		<Field id="firc" desc="金融机构所在地区代码" cached="false" edittype="select" datatype="string" require="true" method="None" readonly="false" translated="CQ:BiAreacodeSelect" editable="true"
		       dropdowntype="dynamic" viewField="areaName"  fieldmap="firc=areacode;fircName=areaName"
		       size="6" status="F" default="" xpath="/firc" />
		<Field id="fircName" desc="金融机构所在地区代码" editable="true" cached="false" status="F"
               method="com.huateng.ebank.business.common.CQMethod.getAreacodeName"
               readonly="" primary="" type="string" size="150"	 xpath="/firc" default="" />	
		<Field id="fict" desc="金融机构代码类型" require="true" edittype="text"
			datatype="string" method="None" readonly="true" size="2" status="F"
			default="" xpath="/fict" />
		<Field id="ficd" desc="金融机构代码" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="12" status="F"
			default="" xpath="/ficd" />
        <Field id="brno" desc="机构号"  require="false" edittype="text" datatype="string" xpath="/brno" />
	   	<Field id="ssdg" desc="可疑程度" require="true" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			translated="DATA_DIC.157" default="" xpath="/ssdg" />
		<Field id="tkms" desc="采取措施" require="false" edittype="textarea"
			datatype="string" method="None" readonly="false" size="192" status="F"
			width="920" default="" xpath="/tkms" />
		<Field id="ssds" desc="可疑行为描述" require="false" edittype="textarea"
			datatype="string" method="None" readonly="false" size="750" status="F"
			width="920" default="" xpath="/ssds" />
			
		<!-- 其他信息 -->       	
        <Field id="xmlnm" desc="XML文件名称" edittype="select"  require="" status="F" translated="CQ:xmlnmGetterSelect" fieldmap="xmlnm=id"
               method="None" viewField="id" primary="true" dropdowntype="dynamic" init="false"  datatype="string" size="64" default="" xpath="/xmlnm" />
        <Field id="zipnm" desc="ZIP文件名称"  require="false" edittype="text" datatype="string" xpath="/zipnm" />
       	<Field id="recStatus" desc="记录状态" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/recStatus" translated="DATA_DIC.9087" tip="记录状态"/>
		<Field id="repStatus" desc="回执状态" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/repStatus" translated="DATA_DIC.301" />
		<Field id="approveStatus" desc="审核状态" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/approveStatus" translated="DATA_DIC.64" tip="审核状态"/>
        <Field id="issubsuccess" desc="是否已成功上报" edittype="select" datatype="string" method="None" readonly="false" 
		       size="1" status="F" default="" xpath="/issubsuccess" translated="DATA_DIC.65"/>
		<Field id="lstUpdTlr" desc="最后更新人" edittype="text" datatype="string" method="None" readonly="false" 
		       size="20" status="F" default="" xpath="/lstUpdTlr" tip="最后更新人"/>
	    <Field id="lstUpdTm" desc="最后更新时间" edittype="text" datatype="timestamp" method="None" readonly="false" 
		       size="14" status="F" default="" xpath="/lstUpdTm" tip="最后更新时间"/>	
		<Field id="filler4" desc="报告创建人" edittype="textarea" datatype="string" method="None" readonly="false" 
			size="256" status="F" default="" xpath="/filler4" tip="报告创建人"/>
			<!--STRAT add 20171122-->
		<Field id="bsid" desc="可疑案件编号" edittype="text" datatype="string" method="None" readonly="false" 
			size="64" status="F" default="" xpath="/bsid" />
			<!--END add 20171122-->
	</Fields>

	<Operations>
        <Button id="btSubmit" name="btSubmit" desc="上报" operation="asysubmit" updateClass="com.huateng.report.hf.aml.bs.update.AmlBSDsCollInfoSubmitUpdate2" url="#" txn="" />
        <Button id="btSave" name="btSave" desc="暂存" operation="asysubmit" updateClass="com.huateng.report.hf.aml.bs.update.AmlBSDsCollInfoUpdate2" url="#" txn="" />
        <Button id="btBack" name="btBack" desc="返回" operation="" url="#" />
		
	</Operations>
</CommQry>
