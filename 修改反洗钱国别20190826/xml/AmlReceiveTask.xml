<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="大额交易" navigate="反洗钱&gt;  大额交易"
	type="call" 
	interface="true" 
	pagesize="100" 
	async="true" 
	databusid=""
	transdataactionurl="/trans/TransDataAction.do" 
	getterclassname="com.huateng.report.hf.aml.getter.AmlReceiveTaskGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示 -->
		<!-- primary : true-作为表单项记录上传 false-不作为表单项上传 -->
		<!--公共基础信息 -->
		<Field id="workDate"              desc="交易发生日期"                         edittype="text" datatype="string"   method="None" readonly="false" size="8"  status="N" default="" xpath="/workDate"              />
		<Field id="custNo"               desc="客户号"                           edittype="text" datatype="string" method="None" readonly="false" size="32"  status="N" default="" xpath="/custNo"               />
		<Field id="interCustSort"        desc="国内客户群体成员"                 edittype="text" datatype="string" method="None" readonly="false" size="4"   status="N" default="" xpath="/interCustSort"        />
		<Field id="orgCode"              desc="交易处理部门"                     edittype="text" datatype="string" method="None" readonly="false" size="30"  status="N" default="" xpath="/orgCode"              />
		<Field id="num" desc="交易数量" edittype="text" datatype="string" method="None" readonly="false" size="100" status="N" default="" xpath="/num" />
		<Field id="tlrno" desc="操作员编号" edittype="text" datatype="string" method="None" readonly="false" size="8" status="N" default="" xpath="/tlrno" />
		<Field id="selectTlrno"  desc="操作员编号"   edittype="select"  require="" status="F" translated="CQ:tlrNoGetterSelect" fieldmap="selectTlrno=id"
               method="None" viewField="id" primary="true" dropdowntype="dynamic" init="false"  datatype="string" size="8" default="" xpath="/selectTlrno" />
		<Field id="select" desc="选择" method="None" edittype="checkbox" datatype="boolean" xpath="" />				
	</Fields>
	<Where>
		<DateTextBox id="qworkDateStart" desc="起始交易日期" edittype="text" datatype="date" size="32" require="true" index="0" />
		<DateTextBox id="qworkDateEnd" desc="结束交易日期" edittype="text" datatype="date" size="32" require="true" index="1" />
		<TextBox id="qcusNo" desc="客户号" edittype="text" status="F" datatype="string" size="64" index="2" />
		<TextBox id="qtlrno" desc="操作员编号" edittype="text" status="F" datatype="string" size="8" index="3" />
	    <Option id="qisReceive" desc="是否分派" edittype="select" status="F"  translated="DATA_DIC.227" datatype="string" size="1" index="4" tip="是否分派"/>
	</Where>
	<Operations>
		<Button id="btReceive" name="btReceive" desc="分派" updateClass="" operation="" 
			url="#" show="" group="" txn="" icon="icon-default"/>
		<Button id="btConfirm" name="btConfirm" desc="确认分派" updateClass="com.huateng.report.hf.aml.update.AmlReceiveTaskUpdate" operation="asysubmit" 
			url="#" show="" group="" txn="" icon=""/>
		<Button id="btUnSave" name="btUnSave" desc="反暂存" updateClass="" operation="" 
			url="#" show="" group="" txn="" icon="icon-undo"/>	
		<Button id="btReceiveAll" name="btReceiveAll" desc="一键分派" updateClass="" operation="" 
			url="#" show="" group="" txn="" icon="icon-default"/>
		<Button id="btConfirm02" name="btConfirm02" desc="确认一键分派" updateClass="" operation="" 
			url="#" show="" group="" txn="" icon="icon-default"/>
	</Operations>
</CommQry>