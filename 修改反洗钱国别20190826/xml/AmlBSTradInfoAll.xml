<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="可疑数据补录" navigate="可疑数据补录&gt;可疑数据补录" type="call"
	interface="true" pagesize="100" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.hf.aml.getter.AmlBSTransGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- Common Field start -->
		<Field id="id" desc="记录主键" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="N"
			default="" primary="true" xpath="/id" />
		<Field id="select" desc="选择" method="None" edittype="checkbox" datatype="boolean" xpath="" />	    
        <Field id="oper" desc="操作" method="None" edittype="text"
            datatype="string" xpath="" />
       <Field id="reportId" desc="内部编号" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="10" status="N" default="" xpath="/reportId"/>   
        <Field id="workDate" desc="工作日期" edittype="text" datatype="date" method="None" readonly="false" require="false"
		       size="8" status="F" default="" xpath="/workDate" />
       <Field id="ctnm" desc="客户名称" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/ctnm" tip="客户名称"/>  
		<Field id="citp" desc="客户身份证件类型" edittype="select" init="false" datatype="string" require="false" method="None" readonly="false" 
		       size="32" translated="DATA_DIC.158" status="F" default="" xpath="/citp" />
		<Field id="citpDesc" desc="客户身份证件/证明文件类型进一步说明" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="32"  status="F" default="" xpath="/citpDesc" /> 
		<Field id="ctid" desc="客户证件号码" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="20" status="F" default="" xpath="/ctid" /> 
		<Field id="ctac" desc="账号" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/ctac"/> 
		 <Field id="bknm" desc="交易代办人姓名" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" xpath="/bknm" />
		<Field id="bitp" desc="代办人身份证件类型" require="false" edittype="select"
			translated="DATA_DIC.158" datatype="string" method="None" readonly="false"
			size="2" status="F" default="" xpath="/bitp" />
		<Field id="bitpDesc" desc="代办人身份证件类型说明" require="false" status="F"
			edittype="text" datatype="string" method="None" size="30" xpath="/bitpDesc" />
		<Field id="bkid" desc="代办人身份证件号码" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="20" status="F"
			default="" xpath="/bkid" />
		<Field id="bknt" desc="代办人国籍"  require="false" edittype="select" size="3"  datatype="string"  
	           status="F" translated="CQ:BiNationregionSelect" editable="true" dropdowntype="dynamic" viewField="nationregionIdName" method="None" fieldmap="bknt=nationregionId;bkntName=nationregionIdName" xpath="/bknt" />
		<Field id="bkntName" desc="代办人国籍" status="F"
               method="com.huateng.ebank.business.common.CQMethod.getNationregionIdName" editable="true"
               readonly="" primary="" type="string" size="150"  xpath="/bknt" default="" />
        <Field id="tstm" desc="交易时间" edittype="text" datatype="date" method="com.huateng.ebank.business.common.CQMethod.getOriginalTimeString" 
		       require="false"  readonly="false" size="14" status="F" default="" xpath="/tstm"/>
        <Field id="ticd" desc="业务标识号" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/ticd" />
		<Field id="tstp" desc="交易方式" edittype="select"  require="false" status="F" translated="CQ:BiTreeDataDicSelect" fieldmap="tstpName=name;tstp=id"
               method="None" viewField="name" primary="true" dropdowntype="dynamic"   datatype="string" size="8" default="" xpath="/tstp" />
        <Field id="tstpName" desc="AmlBHdsColl.field.tstpName" status="F" method="com.huateng.ebank.business.common.CQMethod.getBiTreeDataDicName"
               readonly="" primary="true" type="String" size="150"	 xpath="/tstp" default="" codetype="TSTP"/>
        <Field id="tsct" desc="涉外收支交易分类与代码" require="false" status="F" translated="CQ:MtsInOutCodeTreeSelect"
               fieldmap="tsctName=name;tsct=id" method="None" viewField="name" primary="true"
               dropdowntype="dynamic" edittype="select" datatype="string" size="4" default="" xpath="/tsct" />
        <Field id="tsctName" desc="涉外收支交易分类与代码" status="F" method="com.huateng.ebank.business.common.CQMethod.gettsctName"
               width="" readonly="" primary="true" type="String" size="150" xpath="/tsct" default="" codetype="AML"/>  
        <Field id="tsdr" desc="收付标识" require="false" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/tsdr" translated="DATA_DIC.164" /> 
		<Field id="crsp" desc="资金来源和用途" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="256" status="F"
			default="" xpath="/crsp" />
		<Field id="crtp" desc="币种" editable="true" require="false" edittype="select" translated="CQ:CurrencySelect" dropdowntype="dynamic" method="None" viewField="currencyNoName" status="F"
               fieldmap="crtp=currencyNo;crtpName=currencyNoName"   size="3" default="" xpath="/crtp" tip="币种"/>
	     <Field id="crtpName" desc="币种" editable="true" status="F" require="" method="com.huateng.ebank.business.common.CQMethod.getCurNameByCurNo" width="" readonly="" primary="true" type="String" size="150"
			xpath="/crtp" default=""/>
		 <Field id="crat" desc="交易金额" edittype="text" datatype="currency" require="false" method="None" readonly="false" 
	           size="20" status="F"  scale="2" default="" xpath="/crat" />
	     <Field id="cfin" desc="对方金融机构网点名称" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/cfin"/>
		 <Field id="cfct" desc="对方金融机构代码类型" edittype="select" datatype="string" require="false" method="None" readonly="false" 
		       size="2"  translated="DATA_DIC.9300" status="F" default="" xpath="/cfct" />
 		<Field id="cfic" desc="对方金融机构网点代码" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="12" status="F" default="" xpath="/cfic" />  
		<Field id="tcnm" desc="交易对手姓名" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/tcnm" />
		<Field id="tcit" desc="交易对手证件类型" edittype="select" datatype="string" require="false" method="None" readonly="false" 
		       size="32" translated="DATA_DIC.158" init="false" status="N" default="" xpath="/tcit" /> 
		<Field id="tcitDesc" desc="交易对手证明类型进一步说明" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="32" status="F" default="" xpath="/tcitDesc" />
		<Field id="tcid" desc="交易对手证件号码" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="20" status="F" default="" xpath="/tcid" /> 
		<Field id="tcat" desc="交易对手账户类型" require="false" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/tcat" translated="DATA_DIC.3004" /> 
		<Field id="tcac" desc="交易对手账号" edittype="text" datatype="string" require="false" method="None" readonly="false" 
		       size="64" status="F" default="" xpath="/tcac" />
 	   <Field id="trcd" desc="交易发生地" require="" edittype="select" size="3"  datatype="string"  
	           status="F" translated="CQ:BiNationregionSelect" editable="true" dropdowntype="dynamic" cached="false" viewField="nationregionIdName" method="None" fieldmap="trcd=nationregionId;trcdName=nationregionIdName"
	           xpath="/trcd" />
     <Field id="trcdName" desc="交易发生地" status="F" method="com.huateng.ebank.business.common.CQMethod.getNationregionIdName" editable="true"
               readonly="" primary="" type="string" size="150"	cached="false"  xpath="/trcd" default="" /> 
           
	<Field id="trcdSuffix" desc="交易发生地区代码" edittype="select" datatype="string" require="false" method="None" readonly="false" translated="CQ:BiAreacodeSelect" editable="true"
		       dropdowntype="dynamic" viewField="areaName"  fieldmap="trcdSuffix=areacode;trcdSuffixName=areaName"
		       size="6" status="F" default="" cached="false" xpath="/trcdSuffix" />
	<Field id="trcdSuffixName" desc="交易发生地区代码" editable="true" status="F"
               method="com.huateng.ebank.business.common.CQMethod.getAreacodeName"
               readonly="" primary="" type="string" cached="false" size="150"	 xpath="/trcdSuffix" default="" /> 
               
     <Field id="cfrc" desc="对方金融机构网点地区" require="" edittype="select" size="3"  datatype="string"  
	           status="F" translated="CQ:BiNationregionSelect" editable="true" dropdowntype="dynamic" cached="false" viewField="nationregionIdName" method="None" fieldmap="cfrc=nationregionId;cfrcName=nationregionIdName"
	           xpath="/cfrc" />
     <Field id="cfrcName" desc="对方金融机构网点地区" status="F" method="com.huateng.ebank.business.common.CQMethod.getNationregionIdName" editable="true"
               readonly="" primary="" type="string" size="150"	cached="false"  xpath="/trcd" default="" /> 
           
	<Field id="cfrcSuffix" desc="对方金融机构网点地区行政区划代码" edittype="select" datatype="string" require="false" method="None" readonly="false" translated="CQ:BiAreacodeSelect" editable="true"
		       dropdowntype="dynamic" viewField="areaName"  fieldmap="cfrcSuffix=areacode;cfrcSuffixName=areaName"
		       size="6" status="F" default="" cached="false" xpath="/cfrcSuffix" />
	<Field id="cfrcSuffixName" desc="对方金融机构网点地区行政区划代码" editable="true" status="F"
               method="com.huateng.ebank.business.common.CQMethod.getAreacodeName"
               readonly="" primary="" type="string" cached="false" size="150"	 xpath="/cfrcSuffix" default="" /> 
	</Fields>

	 <Where>
	 	<DateTextBox id="qworkDateStart" desc="开始工作日期" edittype="text" datatype="date" size="32" require="false" index="0" />
    	<DateTextBox id="qworkDateEnd" desc="结束工作日期" edittype="text" datatype="date" size="32" require="false" index="1" />
    	<TextBox id="qticd" desc="业务标识号" edittype="text" status="F" datatype="string" size="128" index="2" />
		<TextBox id="qctac" desc="交易账号" edittype="text" status="F" datatype="string" size="64" index="3" />
    </Where>
	<Operations>
		<Button id="btConfirm" name="btChoose_Cust" desc="确认勾选" updateClass="" operation="" url="#" show="" group="" txn="" icon="icon-ok" />
	</Operations>
</CommQry>
