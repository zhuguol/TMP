<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="可疑数据补录" navigate="可疑数据补录 &gt; 可疑数据补录" type="call" interface="true"
	pagesize="100" async="true" databusid="" transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.hf.aml.getter.AmlBsFileUploadGetter2">
	<Include id="BankParam" />
	<Fields>
		<!-- Common Field start -->
		<Field id="select" desc="选择" method="None" edittype="checkbox" datatype="boolean" xpath="" />	    
		<Field id="id" desc="记录主键" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/id" />
        <Field id="reportId" desc="内部编号" edittype="text" datatype="string" require="false" method="None" readonly="true" 
		       size="10" status="F" default="" xpath="/reportId"/>   
         <Field id="workDate" desc="工作日期" edittype="text" datatype="date" method="None" readonly="true" require="false"
		       size="8" status="F" default="" xpath="/workDate" />
		<Field id="filename" desc="文件路径" require="false" edittype="text"
			datatype="string" method="None" readonly="true" size="500" status="F"
			default="" xpath="/filename" />
	   	<Field id="filesize" desc="文件大小(B)" rrequire="false" edittype="text"
			datatype="string" method="None" readonly="true" size="11" status="F" default="" xpath="/filesize" />
		<Field id="uploadtlr" desc="文件上传人" edittype="text" datatype="string" method="None" readonly="true" 
		       size="20" status="F" default="" xpath="/uploadtlr" tip="最后更新人"/>
	    <Field id="uploadtm" desc="文件上传时间" edittype="text" datatype="timestamp" method="None" readonly="true" 
		       size="14" status="F" default="" xpath="/uploadtm" tip="最后更新时间"/>	
	</Fields>
	<Operations>
		<Button id="btnUpload02" desc="上传附件" url="#" name="btnUpload" icon="icon-impin" txn="" group="" show=""/>
		<Button id="btDel_Files" name="btDel_Files" desc="删除" operation="asysubmit"
			    updateClass="com.huateng.report.hf.aml.bs.update.AmlBSFilesDelUpdate2"
			    url="#" txn="" icon="icon-remove"/>
	</Operations>
</CommQry>
