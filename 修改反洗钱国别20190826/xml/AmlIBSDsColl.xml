<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="可疑数据补录" navigate="可疑数据补录 &gt; 可疑数据补录" type="call" interface="true"
	pagesize="100" async="true" databusid="" transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.hf.aml.getter.AmlIBSdsCollGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- Common Field start -->
		<Field id="id" desc="记录主键" require="false" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/id" />
	<Field id="select" desc="选择" method="None" edittype="checkbox" datatype="boolean" xpath="" />	    
        <Field id="oper" desc="操作" method="None" edittype="text"
            datatype="string" xpath="" />
        <Field id="reportId" desc="内部编号" edittype="text" datatype="string" require="true" method="None" readonly="false" 
		       size="10" status="F" default="" xpath="/reportId"/>   
	   <Field id="reportType" desc="可疑报文类型" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/reportType" translated="DATA_DIC.152" />  
		<Field id="finm" desc="金融机构名称" require="false" edittype="text"
			datatype="string" method="None" readonly="true" size="64" status="F"
			default="" xpath="/finm" />
		<Field id="firc" desc="金融机构所在地区代码" cached="false" edittype="select" datatype="string" require="true" method="None" readonly="false" translated="CQ:BiAreacodeSelect" editable="true"
		       dropdowntype="dynamic" viewField="areaName"  fieldmap="firc=areacode;fircName=areaName"
		       size="6" status="F" default="" xpath="/firc" />
		<Field id="fircName" desc="金融机构所在地区代码" editable="true" cached="false" status="F"
               method="com.huateng.ebank.business.common.CQMethod.getAreacodeName"
               readonly="" primary="" type="string" size="150"	 xpath="/firc" default="" />	
		<Field id="fict" desc="金融机构代码类型" require="true" edittype="text"
			datatype="string" method="None" readonly="true" size="2" status="F"
			default="" xpath="/fict" />
		<Field id="ficd" desc="金融机构代码" require="true" edittype="text"
			datatype="string" method="None" readonly="true" size="12" status="F"
			default="" xpath="/ficd" />
        <Field id="brno" desc="机构号"  require="false" edittype="text" datatype="string" xpath="/brno" />
         <Field id="workDate" desc="工作日期" edittype="text" datatype="date" method="None" readonly="false" require="true"
		       size="8" status="F" default="" xpath="/workDate" />
		<Field id="stcr" desc="可疑特征" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" xpath="/stcr" />
	   	<Field id="ssdg" desc="可疑程度" require="true" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			translated="DATA_DIC.157" default="" xpath="/ssdg" />
		<Field id="tkms" desc="采取措施" require="true" edittype="textarea"
			datatype="string" method="None" readonly="false" size="128" status="F"
			width="735" default="" xpath="/tkms" />
		<Field id="ssds" desc="可疑行为描述" require="true" edittype="textarea"
			datatype="string" method="None" readonly="false" size="500" status="F"
			width="735" default="" xpath="/ssds" />
        <Field id="xmlnm" desc="XML文件名称"  require="false" edittype="text" datatype="string" xpath="/xmlnm" />
        <Field id="zipnm" desc="ZIP文件名称"  require="false" edittype="text" datatype="string" xpath="/zipnm" />
       	<Field id="recStatus" desc="记录状态" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/recStatus" translated="DATA_DIC.9087" tip="记录状态"/>
		<Field id="repStatus" desc="回执状态" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/repStatus" translated="DATA_DIC.301" />
		<Field id="approveStatus" desc="审核状态" edittype="select" datatype="string" method="None" readonly="false" 
		       size="2" status="F" default="" xpath="/approveStatus" translated="DATA_DIC.64" tip="审核状态"/>
        <Field id="issubsuccess" desc="是否已成功上报" edittype="select" datatype="string" method="None" readonly="false" 
		       size="1" status="F" default="" xpath="/issubsuccess" translated="DATA_DIC.65"/>
		<Field id="errMsg" desc="回执内容" require="false" edittype="textarea"
			datatype="string" method="None" readonly="false" size="500" status="F"
			width="735" default="" xpath="/errMsg" />
		<Field id="lstUpdTlr" desc="最后更新人" edittype="text" datatype="string" method="None" readonly="false" 
		       size="20" status="F" default="" xpath="/lstUpdTlr" tip="最后更新人"/>
	    <Field id="lstUpdTm" desc="最后更新时间" edittype="text" datatype="timestamp" method="None" readonly="false" 
		       size="14" status="F" default="" xpath="/lstUpdTm" tip="最后更新时间"/>	
	</Fields>
    <Where>
        <DateTextBox id="qworkDateStart" desc="开始工作日期" edittype="text" datatype="date" size="32" require="false" index="0" />
		<DateTextBox id="qworkDateEnd" desc="结束工作日期" edittype="text" datatype="date" size="32" require="false" index="1" />
        <Option id="qssdg" desc="可疑程度" edittype="select" translated="DATA_DIC.157" status="F" datatype="string" size="2" index="2" />
		<Option id="qrecStatus" desc="记录状态" edittype="select" status="F" translated="DATA_DIC.9087" datatype="string" size="2" index="3" />
		<Option id="qrepStatus" desc="回执状态" edittype="select" status="F" translated="DATA_DIC.301" datatype="string" size="2" index="4" />
    </Where>
	<Operations>
         <Button id="btMod" name="btMod" desc="补录" updateClass="" operation="" url="#" show="" group="" txn="" icon="icon-edit"/>
        <Button id="btReport" name="btReport" desc="上报" updateClass="" operation="" 
			url="#" show="" group="" txn="" icon="icon-upload"/>
		<Button id="btUnReport" name="btUnReport" desc="撤销上报" updateClass="" operation="" 
			url="#" show="" group="" txn="" icon="icon-undo"/>
	</Operations>
</CommQry>
