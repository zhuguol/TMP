<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="resources.bean.report.form">
	<class
		name="CBlackHitCount"
		table="c_black_hit_count"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="searchDate"
			type="string"
			column="SEARCH_DATE"
		></id>
		<property name="queriesNum" column="QUERIES_NUM" type="int" not-null="false" length="20" />
		<property name="hitsNum" column="HITS_NUM" type="int" not-null="false" length="20" />
		<property name="batchTime" column="BATCH_TIME" type="date" not-null="false" length="34" />

	</class>	
</hibernate-mapping>