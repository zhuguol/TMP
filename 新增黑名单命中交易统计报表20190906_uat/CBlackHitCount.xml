		<Field id="hitsNum" desc="Number of Hits" edittype="text" datatype="int" method="None" readonly="false" size="20" status="F" default="" xpath="/hitsNum" />
		<Field id="batchTime" desc="跑批日期" edittype="text" datatype="date" method="None" readonly="false" size="34" status="N" default="" xpath="/batchTime" />
	</Fields>
	<Where>
		<Option id="qlstDateStart" desc="黑名单检索起始时间(年/月)" placeholder="yyyyMM"
			edittype="select" dropdowntype="custom" url="/templets/lib/yyyyMM.ftl"
