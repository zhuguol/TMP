	getterclassname="com.huateng.report.hf.form.getter.CBlackHitCountGetter">
	<Include id="BankParam" />
	<PageQryExp type="XLS" limit="true" encoding="UTF-8" url="/trans/qryExp.do" desc="黑名单命中交易统计报表"  />
	<Fields>
		<Field id="searchDate" desc="Time for Sanction Screening (Year/Month)" edittype="text" datatype="string" method="None" readonly="false" size="10" status="F" default="" xpath="/searchDate" />
