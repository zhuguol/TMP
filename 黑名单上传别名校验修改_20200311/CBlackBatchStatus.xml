<?xml version="1.0" encoding="UTF-8"?>
<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL async:异步提交标志 pagesize:页面显示记录数(缺省为10) -->
<CommQry title="黑名单" navigate="反洗钱&gt;  黑名单"
	type="call" 
	interface="true" 
	pagesize="100" 
	async="true" 
	databusid=""
	transdataactionurl="/trans/TransDataAction.do" 
	getterclassname="com.huateng.report.hf.form.getter.CBlackBatchStatusGetter">
	<Include id="BankParam" />
	<Fields>
		<Field id="etlDate" desc="批处理日期" edittype="text" datatype="date" method="None" readonly="false" size="8" status="F" default="" xpath="/etlDate" />
		<Field id="etlName" desc="批处理名称" edittype="text" datatype="string" method="None" readonly="false" size="200" status="F" default="" xpath="/etlName" />
		<Field id="notice" desc="Notice" edittype="text" datatype="string" method="None" readonly="false" size="400" status="F" default="" xpath="/notice" />
		<Field id="beginTime" desc="批处理开始时间" edittype="text" datatype="timestamp" method="None" readonly="false" size="34" status="F" default="" xpath="/beginTime" />
		<Field id="endTime" desc="批处理结束时间" edittype="text" datatype="timestamp" method="None" readonly="false" size="34" status="F" default="" xpath="/endTime" />
		<!--<Field id="status" desc="批处理状态" edittype="text" datatype="string" method="None" readonly="false" size="2" status="F" default="" xpath="/status" /> -->
        <Field id="status" desc="批处理状态" edittype="select" datatype="string" method="None" readonly="false" size="2" status="F" default=""   translated="LIST:01,完成;02,批处理中"  xpath="/status"   />
		<Field id="transNum" desc="潜在命中交易条数" edittype="text" datatype="int" method="None" readonly="false" size="20" status="F" default="" xpath="/transNum" />
		<Field id="updatePerson" desc="上传的用户" edittype="text" datatype="string" method="None" readonly="false" size="20" status="F" default="" xpath="/updatePerson" />
		<Field id="roleName" desc="上传用户所属部门" edittype="text" datatype="string" method="None" readonly="false" size="30" status="F" default="" xpath="/roleName" />
	</Fields>
	<Where>
		<DateTextBox id="etlDateStart" desc="批处理起始日期" edittype="text" datatype="date" size="34" require="true" index="0" />
		<DateTextBox id="etlDateEnd" desc="批处理终止日期" edittype="text" datatype="date" size="34" require="true" index="1" />
		<TextBox id="notice" desc="Notice" edittype="text" datatype="String" size="400" index="2" />	
	</Where>
	<Operations>
	    <Button id="exportBN" name="exportBN" desc="导出" url="#" icon="icon-upload"/>
	</Operations>
</CommQry>