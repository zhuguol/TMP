<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.bean.report.form">
	<class name="CBlackListUpload" table="c_black_list_upload" >
		<meta attribute="sync-DAO">false</meta>
		 <composite-id name="id" class="CBlackListUploadPk">     
			<key-property name="id" type="string" column="SEQ_NO" />           
			<key-property name="etlDate" type="string" column="ETL_DATE" />	       
		</composite-id>
<!-- 	    <id name="id" type="string" column="SEQ_NO" > </id>
	    
		<property name="etlDate" column="ETL_DATE" type="string" not-null="false" length="8" /> -->
		<property name="serial" column="SERIAL" type="string" not-null="false" length="100" />
		<property name="issuPboc" column="ISSU_PBOC" type="string"  not-null="false" length="20" />
		<property name="notice" column="NOTICE" type="string" not-null="false" length="400" />
		<property name="targetName" column="TARGET_NAME" type="string"  not-null="false" length="4000" />
		<property name="dob" column="DOB" type="string" not-null="false" length="400" />
		<property name="alias" column="ALIAS" type="string" not-null="false" length="900" />
		<property name="nationality" column="NATIONALITY" type="string" not-null="false" length="4000" />
		<property name="others" column="OTHERS" type="string" not-null="false" length="400" />
		<property name="worldCheck" column="WORLD_CHECK" type="string" not-null="false" length="100" />
		<property name="remark" column="REMARK" type="string" not-null="false" length="400" />
		<property name="updatePerson" column="UPDATE_PERSON" type="string" not-null="false" length="20" />
	</class>
</hibernate-mapping>