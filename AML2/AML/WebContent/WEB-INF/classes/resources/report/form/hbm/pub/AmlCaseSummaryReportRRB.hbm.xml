<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.bean.report.form">
	<class name="AmlCaseSummaryReportRRB" table="AML_CASE_SUMMARY_REPORT_RRB">
		<meta attribute="sync-DAO">false</meta>
		<id name="seqNo" type="big_decimal" column="SEQ_NO"></id>
		<property name="id" column="ID" type="string" not-null="false" length="12"/>
		<property name="idDesc" column="ID_DESC" type="string" not-null="false" length="128"/>
		<property name="sumNum" column="SUM_NUM" type="string" not-null="false" length="40"/>
		<property name="dataDate" column="DATA_DATE" type="string" not-null="false" length="6"/>
	</class>	
</hibernate-mapping>
