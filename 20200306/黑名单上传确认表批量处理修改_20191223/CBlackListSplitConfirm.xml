			url="#" txn="" />
		<Button id="confirmBlackName" name="confirmBlackName" desc="提交" operation="asysubmit"
			updateClass="com.huateng.report.hf.aml.update.CBlackListSplitConfirmSelect"
			url="#" txn="" />
		<Button id="confirmSplitYes" name="confirmSplitYes" desc="全部确认" url="#" /> 
<!-- 		<Button id="confirmSplitNo" name="confirmSplitNo" desc="按条件确认错误" url="#" />  -->
