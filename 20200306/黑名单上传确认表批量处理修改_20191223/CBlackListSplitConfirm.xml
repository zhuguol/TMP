         <Button id="selectYes" name="selectYes" desc="批量确认"  url="#" />
         <Button id="confirmBNSelect" name="confirmBNSelect" desc="提交" operation="asysubmit"
			updateClass="com.huateng.report.hf.aml.update.CBlackListSplitConfirmSelectUpdate"
			url="#" txn="" />
