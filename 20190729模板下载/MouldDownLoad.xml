<?xml version="1.0" encoding="UTF-8"?>
<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="" navigate="主页 &gt; 模板下载 &gt; 模板下载" type="call"
	interface="false" pagesize="10" async="false" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.export.getter.MouldDownLoadSelectGetter">
	<Include id="BankParam" />
	<Fields>
		<Field id="select" desc="选择" method="None" edittype="checkbox" datatype="boolean" xpath="" />	  
        <Field id="id" desc="序号" status="N" readonly="true"
			primary="true" type="String" size="30" xpath="/id" />
			
		<Field id="fileEName" desc="文件英文名称" status="N" readonly="true"
			primary="true" type="String" size="64" xpath="/fileEName" />
			
		<Field id="fileCName" desc="文件中文名称" status="N" readonly="true"
			primary="true" type="String" size="64" xpath="/fileCName" />
			
		<Field id="status" desc="是否有效" status="F" method="None"
			edittype="select" translated="LIST:1,1-有效;2,2-无效;" readonly="false"
			primary="false" type="String" size="2" xpath="/status" />
		
		<Field id="fileType" desc="文件类型" status="N" readonly="true"
			primary="true" type="String" size="10" xpath="/fileType" />
				
		<Field id="upDepartment" desc="上传部门" status="N" readonly="true"
			primary="true" type="String" size="10" xpath="/upDepartment" />
		
		<Field id="upTime" desc="上传时间" status="N" readonly="true"
			primary="true" type="String" size="8" xpath="/upTime" />
			
		<Field id="filler1" desc="备用字段1" status="N" readonly="true"
			primary="true" type="String" size="64" xpath="/filler1" />
			
		<Field id="filler2" desc="备用字段2" status="N" readonly="true"
			primary="true" type="String" size="64" xpath="/filler2" />
			
		<Field id="filler3" desc="备用字段3" status="N" readonly="true"
			primary="true" type="String" size="64" xpath="/filler3" />
		<Field id="oper" desc="操作" />

	</Fields>
	<Where>
		<DateTextBox id="openTimeStart" desc="起始日期" edittype="text" datatype="date" size="32" require="false" index="0" /> 
  		<DateTextBox id="openTimeEnd" desc="截止日期" edittype="text" datatype="date" size="32" require="false" index="1" />
	    <TextBox id="qfileEName" desc="文件英文名称" edittype="text" status="F" datatype="string" size="64" index="2" />
	    <TextBox id="qfileCName" desc="文件中文名称" edittype="text" status="F" datatype="string" size="64" index="3" />
	    <TextBox id="qupDepartment" desc="文件上传部门" edittype="text" status="F" datatype="string" size="64" index="4" />
	    <Option id="qstatus" desc="是否有效" edittype="select" status="F" translated="LIST:1,1-有效;2,2-无效;"  datatype="string" size="2" index="5" />
	</Where>

	<Operations>
		<Button id="btAddMould" name="btAddMould" desc="新增" operation=""
			url="#" show="" group="" txn="" icon="icon-add"/>
		<Button id="btModMould" name="btModMould" desc="修改" operation=""
			url="#" show="" group="" txn="" icon="icon-set"/>
		<Button id="btRemMould" name="btRemMould" desc="删除" 
			url="#" show="" group="" txn="" icon="icon-remove"/>
	</Operations>
</CommQry>