<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.hsbc.creditcard.bean">
	<class
		name="CreditCardAlert"
		table="CREDIT_CARD_ALERT"
	>
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="string" column="REC_ID">
		</id>

		<property name="alarmDate"         column="ALARM_DATE"          type="string" not-null="false" length="8"   />
		<property name="alarmNo"           column="ALARM_NO"            type="string" not-null="false" length="52"  />
		<property name="ruleName"          column="RULE_NAME"           type="string" not-null="false" length="52"  />
		<property name="riskLevel"         column="RISK_LEVEL"          type="string" not-null="false" length="1"   />
		<property name="isAlertm"          column="IS_ALERT_M"          type="string" not-null="false" length="1"   />
		<property name="custNo"            column="CUST_NO"             type="string" not-null="false" length="52"  />
		<property name="custName"          column="CUST_NAME"           type="string" not-null="false" length="52"  />
		<property name="custGender"        column="CUST_GENDER"         type="string" not-null="false" length="1"  />
		<property name="custCerttype"      column="CUST_CERT_TYPE"      type="string" not-null="false" length="52"  />
		<property name="custCertno"        column="CUST_CERT_NO"        type="string" not-null="false" length="52"  />
		<property name="custCategory"      column="CUST_CATEGORY"       type="string" not-null="false" length="52"  />
		<property name="custPhoneno"       column="CUST_PHONE_NO"       type="string" not-null="false" length="52"  />
		<property name="custWorkunit"      column="CUST_WORK_UNIT"      type="string" not-null="false" length="52"  />
		<property name="custIncome"        column="CUST_INCOME"         type="string" not-null="false" length="52"  />
		<property name="creditCardcny"     column="CREDIT_CARD_CNY"     type="string" not-null="false" length="52"  />
		<property name="activeTimecny"     column="ACTIVE_TIME_CNY"     type="string" not-null="false" length="14"  />
		<property name="creditCardusd"     column="CREDIT_CARD_USD"     type="string" not-null="false" length="52"  />
		<property name="activeTimeusd"     column="ACTIVE_TIME_USD"     type="string" not-null="false" length="14"  />
		<property name="limitCny"          column="LIMIT_CNY"           type="big_decimal" not-null="false" length="20"  />
		<property name="limitAvailablecny" column="LIMIT_AVAILABLE_CNY" type="big_decimal" not-null="false" length="20"  />
		<property name="billingDate"       column="BILLING_DATE"        type="string" not-null="false" length="8"   />
		<property name="billingAddress"    column="BILLING_ADDRESS"     type="string" not-null="false" length="520" />
		<property name="repayDate"         column="REPAY_DATE"          type="string" not-null="false" length="8"   />
		<property name="pbcScore"          column="PBC_SCORE"           type="string" not-null="false" length="52"  />
		<property name="firstReducetime"   column="FIRST_REDUCE_TIME"   type="string" not-null="false" length="14"  />
		<property name="transactorNo"      column="TRANSACTOR_NO"       type="string" not-null="false" length="52"  />
		<property name="proofResult"       column="PROOF_RESULT"        type="string" not-null="false" length="2"   />
		<property name="merchantResult"    column="MERCHANT_RESULT"     type="string" not-null="false" length="2"   />
		<property name="outboundResult"    column="OUTBOUND_RESULT"     type="string" not-null="false" length="2"   />
		<property name="investigateResult" column="INVESTIGATE_RESULT"  type="string" not-null="false" length="2"   />
		<property name="investigateReport" column="INVESTIGATE_REPORT"  type="string" not-null="false" length="500" />
		<property name="fcUar"             column="FC_UAR"              type="string" not-null="false" length="52"  />
		<property name="messageResult"     column="MESSAGE_RESULT"      type="string" not-null="false" length="2"   />
		<property name="billStage"         column="BILL_STAGE"          type="string" not-null="false" length="2"   />
		<property name="reduce"            column="REDUCE"              type="string" not-null="false" length="1"   />
		<property name="cardFreeze"        column="CARD_FREEZE"         type="string" not-null="false" length="2"   />
		<property name="approveStatus"     column="APPROVE_STATUS"      type="string" not-null="false" length="2"   />
		<property name="approveMemo"       column="APPROVE_MEMO"        type="string" not-null="false" length="500" />
		<property name="approveTlr"        column="APPROVE_TLR"         type="string" not-null="false" length="30"  />
		<property name="operatorTlr"       column="OPERATOR_TLR"        type="string" not-null="false" length="30"  />
		<property name="openAlerttime"     column="OPEN_ALERT_TIME"     type="string" not-null="false" length="14"  />
		<property name="closeAlerttime"    column="CLOSE_ALERT_TIME"    type="string" not-null="false" length="14"  />
		<property name="reOpenalertTime"   column="RE_OPEN_ALERT_TIME"  type="string" not-null="false" length="14"  />
		<property name="reClosealertTime"  column="RE_CLOSE_ALERT_TIME" type="string" not-null="false" length="14"  />
		<property name="recStatus"         column="REC_STATUS"          type="string" not-null="false" length="2"   />
		<property name="checkFlag"         column="CHECK_FLAG"          type="string" not-null="false" length="1"   />
		<property name="rsv1"              column="RSV1"                type="string" not-null="false" length="10"  />
		<property name="rsv2"              column="RSV2"                type="string" not-null="false" length="10"  />
		<property name="rsv3"              column="RSV3"                type="string" not-null="false" length="50"  />
		<property name="rsv4"              column="RSV4"                type="string" not-null="false" length="50"  />
		<property name="rsv5"              column="RSV5"                type="string" not-null="false" length="500" />
		<property name="rsv6"              column="RSV6"                type="string" not-null="false" length="500" />
		
	</class>	
</hibernate-mapping>