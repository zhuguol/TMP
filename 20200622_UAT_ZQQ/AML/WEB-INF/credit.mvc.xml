<?xml version="1.0"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<!-- 文件上传控制 -->
	<bean id="apmMultipartResolver"
		  class="org.springframework.web.multipart.commons.CommonsMultipartResolver">
		  <property name="maxUploadSize">
			<value>104857600</value>
		 </property>
		 <property name="maxInMemorySize">
			<value>4096</value>
		 </property>
	</bean>
	
	
	<bean id="batchImportController" class="com.huateng.excel.imp.BatchImportController">
		<property name="multipartResolver" ref="apmMultipartResolver"></property>
		<property name="result" value="/fpages/aml/jsp/message.jsp"/>
		<property name="methodNameResolver">
			<bean class="org.springframework.web.servlet.mvc.multiaction.PropertiesMethodNameResolver">
				<property name="mappings">
					<props>				
						<prop key="/batchImport/upload.spring">uploadImport</prop>
						<prop key="/batchImport/downloadproof.spring">downloadProofFile</prop>
					</props>
				</property>
			</bean>
		</property>
	</bean> 
	
		<!-- 地址映射 -->
	<bean id="apmUrlMapping"
		  class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		  <property name="mappings">
			<props>
				<prop key="/batchImport/*">batchImportController</prop>
			</props>
		 </property>
	</bean>
	<!--<bean id="batchImportService" class="com.huateng.excel.imp.BatchImportService">
    </bean>-->
	
	
	
	
	 <!-- add 20170508 HSBC AML   可疑交易导入 start-->
    <bean id="amlBatchImportController" class="com.huateng.report.hf.aml.imp.AmlBatchImportController">
		<property name="multipartResolver" ref="apmMultipartResolver"></property>
		<property name="result" value="/fpages/hf/jsp/message.jsp"/>
		<property name="resultWithMail" value="/fpages/hf/jsp/messageWithMail.jsp"/>
		<property name="methodNameResolver">
			<bean class="org.springframework.web.servlet.mvc.multiaction.PropertiesMethodNameResolver">
				<property name="mappings">
					<props>				
						<prop key="/amlBatchImport/upload.spring">uploadImport</prop>
						<prop key="/amlBatchImport/upload01.spring">uploadImport01</prop>
						<prop key="/amlBatchImport/import.spring">artificialCorrectionImport</prop>
						<prop key="/amlBatchImport/uploadFileSftp.spring">uploadFileSftp</prop>
						<prop key="/amlBatchImport/mailTest.spring">mailTest</prop>
						<prop key="/amlBatchImport/uploadExchangeWithinCounter.spring">uploadExchangeWithinCounter</prop>
						<prop key="/amlBatchImport/uploadFiles.spring">uploadFilsesImport</prop>
						<prop key="/amlBatchImport/uploadBN.spring">uploadBlackName</prop>
					</props>
				</property>
			</bean>
		</property>
	</bean> 
	
	<bean id="amlApmUrlMapping"
		  class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		  <property name="mappings">
			<props>
				<prop key="/amlBatchImport/*">amlBatchImportController</prop>
			</props>
		 </property>
	</bean>
	 <!-- add 20170508 HSBC AML   end-->
	 
	 
	 
	 
	<bean id="hfaml3"
		  class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		  <property name="mappings">
			<props>
				<prop key="/receiptOrNotice/*">receiptOrNoticeController</prop>
				<prop key="/BhOrBsImport/*">BhOrBsImportController</prop>
				<prop key="/StcrOrFincImport/*">StcrOrFincImportController</prop>
				<prop key="/warnReceiptImport/*">warnReceiptController</prop>
				<!-- <prop key="/analysis/*">receiptAnalysisController</prop> -->
			</props>
		 </property>
	</bean>
	
	  <!-- add 20181031 HSBC AML3号令  回执和人工补正通知导入    start-->
    <bean id="receiptOrNoticeController" class="com.huateng.report.hfaml3.controller.ReceiptOrNoticeController">
		<property name="multipartResolver" ref="apmMultipartResolver"></property>
		<property name="manualCorrectionService" ref="manualCorrectionService"></property>
		<property name="result" value="/fpages/hfaml3/jsp/message.jsp"/>
		<property name="methodNameResolver">
			<bean class="org.springframework.web.servlet.mvc.multiaction.PropertiesMethodNameResolver">
				<property name="mappings">
					<props>				
						<prop key="/receiptOrNotice/importReceiptOrNotice.spring">importReceiptOrNotice</prop>
					</props>
				</property>
			</bean>
		</property>
	</bean> 
	 <!-- add 20181031 HSBC AML   end-->
	 
	  <!-- add 20181109 HSBC AML3号令  大额和可疑交易导入    start-->
